{
    "editor.occurrencesHighlight": false,
    "clangd.checkUpdates": false,
	"code-runner.enableAppInsights": false,
	"docker-explorer.enableTelemetry": false,
	"extensions.ignoreRecommendations": true,
	"gitlens.showWelcomeOnInstall": false,
	"gitlens.showWhatsNewAfterUpgrades": false,
	"java.help.firstView": "none",
	"java.help.showReleaseNotes": false,
	"julia.enableTelemetry": false,
	"kite.showWelcomeNotificationOnStartup": false,
	"liveServer.settings.donotShowInfoMsg": true,
	"Lua.telemetry.enable": false,
	"material-icon-theme.showWelcomeMessage": false,
	"pros.showWelcomeOnStartup": false,
	"pros.useGoogleAnalytics": false,
	"redhat.telemetry.enabled": false,
	"rpcServer.showStartupMessage": false,
	"shellcheck.disableVersionCheck": true,
	"sonarlint.disableTelemetry": true,
	"telemetry.enableCrashReporter": false,
	"telemetry.enableTelemetry": false,
	"telemetry.telemetryLevel": "off",
	"terraform.telemetry.enabled": false,
	"update.showReleaseNotes": false,
	"vsicons.dontShowNewVersionMessage": true,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": false,
    "vim.disableExtension": true,
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    // Yes, I'm smart enough to not donwload garbage in the first place, but once I've
    // downloaded something I would like to be able to just open it
    "security.workspace.trust.enabled": false,
    // Never worked in the first place
    "nim.enableNimsuggest": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "nim.lintOnSave": false,
    "githubPullRequests.queries": [
        {
            "label": "Waiting For My Review",
            "query": "is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "is:open assignee:${user}"
        },
        {
            "label": "All PRs",
            "query": "sort:updated-desc"
        },
        {
            "label": "Created By Me",
            "query": "is:open author:${user}"
        }
    ],
    // Stop the buzz
    "extensions.ignoreRecommendations": true,
    "update.mode": "none",
    "csharp.suppressDotnetInstallWarning": true,
    "workbench.colorTheme": "High Contrast Theme",
    // What else?
    "cmake.configureOnOpen": false,
    "editor.fontFamily": "'Iosevka', 'monospace', monospace",
    // Because neovim extension is used
    "editor.lineNumbers": "relative",
    "files.autoSave": "onFocusChange",
    "[cpp]": {
        "editor.formatOnSave": false,
        "editor.formatOnPaste": false,
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[python]": {
        "editor.formatOnSave": false,
        "editor.tabSize": 4,
        "formatting.provider": "yapf",
        "editor.defaultFormatter": "yapf"
    },
    "python.formatting.provider": "yapf",
    "[js]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    // In most cases it is pretty useless
    "editor.minimap.enabled": false,
    "redhat.telemetry.enabled": false,
    // Automatically pick up schema files from the workspace roots if possible
    "xml.fileAssociations": [
        {
            "pattern": "**/*.xml",
            "systemId": "schema.xsd"
        }
    ],
    "githubPullRequests.terminalLinksHandler": "github",
    "xml.server.vmargs": "-Xmx128M",
    "workbench.tree.indent": 4,
    "editor.tabSize": 2,
    "githubPullRequests.terminalLinksHandler": "github",
    "xml.server.vmargs": "-Xmx128M",
    "workbench.tree.indent": 4,
    "editor.tabSize": 2,
    "githubPullRequests.commentExpandState": "collapseAll",
    "cmake.ignoreCMakeListsMissing": true,
    "files.insertFinalNewline": true,
    "todo-tree.filtering.ignoreGitSubmodules": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "QUESTION",
        "NOTE",
        "IMPLEMENT",
        "TEMP"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "githubPullRequests.pullBranch": "never",
    "editor.renderWhitespace": "none",
    "C_Cpp.default.compilerPath": "/bin/clang++",
    "cmake.configureOnEdit": false,
    "vue.features.codeActions.savingTimeLimit": 5000,
    "terminal.integrated.scrollback": 50000,
    "git.openRepositoryInParentFolders": "never",
    "vue.codeActions.savingTimeLimit": 10000,
    "files.associations": {
        "*.test": "Gherkin"
    },
    "cSpell.enabled": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "markdown.preview.scrollEditorWithPreview": false,
    "[javascript]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "terminal.integrated.inheritEnv": false,
    "workbench.editor.languageDetection": false,
    "workbench.editor.historyBasedLanguageDetection": false,
    "workbench.editor.tabSizing": "fixed",
    "workbench.editor.tabSizingFixedMaxWidth": 120,
    "workbench.editor.tabSizingFixedMinWidth": 120,
    "svelte.enable-ts-plugin": true,
    "editor.minimap.enabled": false,
    "[typescript]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "git.confirmSync": false,
    "codemap.test": [
        {
            "pattern": "^(\\s*)Scenario (.*)",
            "clear": "^(\\s*)Scenario ",
            "prefix": "",
            "icon": "level2",
            "levelIndent": 0
        }, 
        {
            "pattern": "^(\\s*)Environment\\s?(.*)",
            "clear": "^(\\s*)Environment",
            "prefix": "",
            "icon": "level1",
            "levelIndent": 0
        }
    ]
}
